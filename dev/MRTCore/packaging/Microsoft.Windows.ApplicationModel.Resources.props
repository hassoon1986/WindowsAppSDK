<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <ItemGroup Condition="'$(TargetFrameworkIdentifier)'=='.NETCoreApp' and '$(EnableDefaultItems)'=='true' and '$(EnableDefaultContentItems)'!='false' and '$(EnableCoreMrtTooling)'=='true' and '$(DefaultItemExcludes)'!=''">
    <Content Include="$(_ReunionImageGlobs)" Exclude="$(DefaultItemExcludes.Replace(`$(_ReunionImageGlobs)`, ``));$(DefaultExcludesInProjectFolder)" />
    <PRIResource Include="**/*.resw" Exclude="$(DefaultItemExcludes.Replace(`**/*.resw`, ``))');$(DefaultExcludesInProjectFolder)" />
  </ItemGroup>

</Project>
